   "name": "large-comments",
   "displayName": "Large Comments",
   "description": "Large comments generator",
   "version": "0.0.7",
   "engines": {
      "vscode": "^1.58.1"
   },
   "categories": [
      "Programming Languages",
      "Snippets"
