{
   "name": "large-comments",
   "displayName": "Large Comments",
   "description": "Large comments generator",
   "version": "0.0.2",
   "engines": {
      "vscode": "^1.58.0"
   },
   "categories": [
      "Other"
   ],
   "activationEvents": [
      "onCommand:large-comments.large-comments"
   ],
   "main": "./out/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "large-comments.large-comments",
            "title": "Generate large comments"
         }
      ],
      "keybindings": [
         {
            "command": "large-comments.large-comments",
            "key": "ctrl+alt+/",
            "mac": "cmd+alt+/"
         }
      ]
   },
   "publisher": "elgs",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/elgs/large-comments.git"
   },
   "scripts": {
      "vscode:prepublish": "npm run -S esbuild-base -- --minify",
      "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
      "esbuild": "npm run -S esbuild-base -- --sourcemap",
      "esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch",
      "test-compile": "tsc -p ./",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
   },
   "devDependencies": {
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.2.2",
      "@types/node": "14.x",
      "@types/vscode": "^1.58.0",
      "@typescript-eslint/eslint-plugin": "^4.26.0",
      "@typescript-eslint/parser": "^4.26.0",
      "esbuild": "^0.12.15",
      "eslint": "^7.27.0",
      "glob": "^7.1.7",
      "mocha": "^8.4.0",
      "typescript": "^4.3.2",
      "vscode-test": "^1.5.2"
   },
   "dependencies": {
      "figlet": "^1.5.0"
   }
}