{
   "name": "large-comments",
   "displayName": "Large Comments",
   "description": "Large comments generator",
   "version": "0.0.6",
   "engines": {
      "vscode": "^1.58.1"
   },
   "categories": [
      "Programming Languages",
      "Snippets"
   ],
   "activationEvents": [
      "onCommand:large-comments.large-comments"
   ],
   "main": "./out/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "large-comments.large-comments",
            "title": "Generate large comments"
         }
      ],
      "keybindings": [
         {
            "command": "large-comments.large-comments",
            "key": "ctrl+alt+/",
            "mac": "cmd+alt+/"
         }
      ]
   },
   "publisher": "elgs",
   "keywords": [
      "comments",
      "ascii",
      "art"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/elgs/large-comments/"
   },
   "icon": "images/icon.png",
   "scripts": {
      "vscode:prepublish": "npm run -S esbuild-base -- --minify",
      "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
      "esbuild": "npm run -S esbuild-base -- --sourcemap",
      "esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch",
      "test-compile": "tsc -p ./",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
   },
   "devDependencies": {
      "@types/glob": "^7.1.4",
      "@types/mocha": "^8.2.3",
      "@types/node": "16.x",
      "@types/vscode": "^1.58.1",
      "@typescript-eslint/eslint-plugin": "^4.28.3",
      "@typescript-eslint/parser": "^4.28.3",
      "esbuild": "^0.12.15",
      "eslint": "^7.31.0",
      "glob": "^7.1.7",
      "mocha": "^9.0.2",
      "typescript": "^4.3.5",
      "vscode-test": "^1.5.2"
   },
   "dependencies": {
      "figlet": "^1.5.0"
   }
}