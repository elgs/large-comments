   "activationEvents": [
      "onCommand:large-comments.large-comments",
      "onCommand:large-comments.boxed-comments"
   ],
   "main": "./out/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "large-comments.boxed-comments",
            "title": "Generate boxed comments"
         },
         {
            "command": "large-comments.large-comments",
            "title": "Generate large comments"
         }
      ],
      "keybindings": [
         {
            "command": "large-comments.boxed-comments",
            "key": "ctrl+alt+/",
            "mac": "cmd+alt+/"
         },
         {
            "command": "large-comments.large-comments",
            "key": "ctrl+alt+shift+/",
            "mac": "cmd+ctrl+shift+/"
         }
      ],
      "configuration": {
         "title": "LargeComments",
